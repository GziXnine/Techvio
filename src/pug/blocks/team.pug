include ../components/variable

section.team
  block header
    .head
      h2 Team Member
      p Expert Team
  .container
    .row
      each member in teamMembers
        .col-lg-3.col-md-6.col-sm-12.lead
          div.photo
            img(src=`assets/images/team/${member.img}`, alt="team")
            .data
              a(href="#"): i.fa-brands.fa-facebook-f
              a(href="#"): i.fa-brands.fa-x-twitter
              a(href="#"): i.fa-brands.fa-linkedin
          h4.mt-15 #{member.name}
          p #{member.role}